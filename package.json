{
  "name": "rflask-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate project for React & Flask",
  "engines": {
    "npm": "6.14.13",
    "node": "14.17.3"
  },
  "scripts": {
    "build": "cross-env APP_ENV=production concurrently --kill-others-on-fail npm:build:*",
    "build:assets": "cpx \"src/assets/**/*.*\" dist/assets",
    "build:frontend": "webpack --output-path dist/public --config src/apps/frontend/webpack.prod.js",
    "lint": "eslint .",
    "lint:md": "remark .",
    "lint:fix": "eslint --fix .",
    "serve": "cross-env concurrently --kill-others-on-fail npm:serve:*",
    "serve:assets": "cpx \"src/assets/**/*.*\" dist/assets --watch",
    "serve:backend": "cross-env make run-engine-winx86",
    "serve:frontend": "webpack serve --output-path dist/public --config src/apps/frontend/webpack.dev.js --hot --progress",
    "start": "cross-env npm run serve:backend",
    "test": "cross-env APP_ENV=testing make run-test"
  },
  "author": "jalantechnologies",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.0.0",
    "bootstrap": "^5.1.3",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "cpx": "^1.5.0",
    "ejs": "^3.1.6",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@faker-js/faker": "^6.3.1",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.4",
    "@types/chai-http": "^4.2.0",
    "@types/config": "0.0.41",
    "@types/lodash": "^4.14.186",
    "@types/mocha": "^9.1.0",
    "@types/react": "^18.0.21",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "forever": "^4.0.3",
    "html-webpack-plugin": "^5.5.0",
    "js-yaml": "^4.1.0",
    "mocha": "^9.2.0",
    "nyc": "^15.1.0",
    "remark-cli": "^10.0.0",
    "remark-lint": "^9.1.0",
    "remark-preset-lint-recommended": "^6.1.1",
    "sass": "^1.54.9",
    "sass-loader": "^13.0.2",
    "sinon": "^13.0.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "ts-mocha": "^9.0.2",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.4.4",
    "wait-on": "^7.0.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
